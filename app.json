{
  "expo": {
    "name": "TuneFlip",
    "slug": "tuneflip",
    "version": "1.0.0",
    "scheme": "tuneflip",
    "android": {
      "icon": "./assets/tuneflip-icon-1024.png",
      "package": "com.alonso.tuneflip",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "https",
              "host": "tuneflip.app",
              "pathPrefix": "/track"
            }
          ]
        }
      ]
    },
    "ios": {
      "icon": "./assets/tuneflip-icon-1024.png",
      "associatedDomains": [
        "applinks:tuneflip.app"
      ]
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b8b54ac3-8372-4c80-8b3f-22dedb3194c4",
      "enabled": true
    },
    "extra": {
      "eas": {
        "projectId": "b8b54ac3-8372-4c80-8b3f-22dedb3194c4"
      }
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
